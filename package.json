  "type": "module",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "next dev -p 3002",
    "build": "next build",
    "start": "next start",
    "generateOgImages": "node --loader ts-node/esm --es-module-specifier-resolution=node ./scripts/generate_og_images.ts",
    "export": "yarn run generateOgImages && next build && next export",
