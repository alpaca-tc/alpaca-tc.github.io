    "@tailwindcss/typography": "^0.5.9",
    "dayjs": "^1.11.7",
    "gray-matter": "^4.0.3",
    "next": "^13.1.6",
    "puppeteer-core": "19.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
