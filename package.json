    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn run lint:fix"
    ]
  },
